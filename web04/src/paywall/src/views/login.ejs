<%- include('header') %>


<form action="/login" method="post" class="my-3 col-lg-4 col-md-6 mx-auto">
    <div class="form-group">
        <label for="username" class="my-2">Username</label>
        <input type="text" class="form-control my-2" id="username" name="username" required>
    </div>
    <div class="form-group">
        <label for="password" class="my-2">Password</label>
        <input type="password" class="form-control my-2" id="password" name="password" required>
    </div>
    <button type="submit" class="btn btn-primary my-2 w-100">Login</button>
    <button type="submit" class="btn btn-primary my-2 w-100">Register</button>
</form>

<%- include('footer') %>
